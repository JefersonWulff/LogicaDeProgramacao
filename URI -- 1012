#include <stdio.h>
int main() {
    double A, B, C, PI, areaTRI, areaC, areaQ, areaR, areaTRA;
    PI = 3.14159;
    
    scanf("%lf %lf %lf", &A, &B, &C);
    
    areaTRI = (A * C)/2;
    areaC = PI * C * C;
    areaQ = B * B;
    areaR = A * B;
    areaTRA = (A + B)* C /2;
    
    printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n", areaTRI, areaC, areaTRA, areaQ, areaR);
    return 0;
    
}
