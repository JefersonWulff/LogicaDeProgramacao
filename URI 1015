#include <stdio.h>
#include <math.h>
int main() {
    double X1, X2, Y1, Y2, DISTANCIA, SOMA;
    
    scanf("%lf %lf", &X1, &Y1);
    scanf("%lf %lf", &X2, &Y2);
    
    SOMA = (X2 - X1) * (X2 - X1) + (Y2 - Y1) * (Y2 - Y1);
    DISTANCIA = sqrt(SOMA);
    
    printf("%.4lf\n", DISTANCIA);
    return 0;
    
}
