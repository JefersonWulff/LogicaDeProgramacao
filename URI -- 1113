#include <stdio.h>

int main(){

    int A,B;
    
    
    for(;1;){
        scanf("%d %d", &A, &B);
        
        if(B > A){
            printf("Crescente\n");
        }else if(A > B){
            printf("Decrescente\n");
        
        }else if (A==B) 
            break;
        }

    return 0;
}
